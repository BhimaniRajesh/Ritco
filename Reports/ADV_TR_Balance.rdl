      <Query>
        <DataSourceName>SSRS</DataSourceName>
        <CommandText>select top 1  @BRCD=(case when @BRCD=loccode and @Cumulative='Y' then 'All' else @BRCD end) from webx_location where loc_level=1 and activeflag='Y'
--select @BRCD,loccode from webx_location where loc_level=1 and activeflag='Y'
declare @SQL nvarchar(max),@SQL_OPN varchar(6000),@SQL_STR varchar(2000),@TYP_SubRQ varchar(500)
declare @TYP varchar(500),@whereACC varchar(500),@QR_join varchar(2500),@TYP_Select varchar(5000)

--select @RPT_SUB=(case when @Cumulative='Y' then '2' else '0' end)
select @SQL_OPN='',@SQL_STR ='',@whereACC='',@QR_join=''
declare @Visible varchar(10)
      select @Visible ='T'
if  @RPT_SUB='1' and  @RPT='3'
      select @Visible ='F'

if  @RPT_SUB='2' and  @RPT='2'
      select @Visible ='F'

if @Visible ='T'
begin
if @acccode&lt;&gt;'All' 
	select @whereACC=@whereACC+' and ( company_acccode in ('''+replace(@acccode,',',''',''')+''') or a.acccode in ('''+replace(@acccode,',',''',''')+''')) '
if @GRP_STATUS&lt;&gt;'All' 
	select @whereACC=@whereACC+' and Main_category in ('''+replace(@GRP_STATUS,',',''',''')+''')'
if @CUST_VENDCD&lt;&gt;'All' 
	select @whereACC=@whereACC+' and pbov_code in ('''+replace(@CUST_VENDCD,',',''',''')+''')'
if @Groupcode&lt;&gt;'All' 
	select @whereACC=@whereACC+' and g.groupcode in ('''+replace(@Groupcode,',',''',''')+''')'
if @BRCD&lt;&gt;'All'  and @Cumulative='Y'
	select @whereACC=@whereACC+' and ( l.loccode in ('''+replace(@BRCD,',',''',''')+''') or l.report_loc in ('''+replace(@BRCD,',',''',''')+''') )'
if @BRCD&lt;&gt;'All'  and @Cumulative='N'
	select @whereACC=@whereACC+' and ( l.loccode in ('''+replace(@BRCD,',',''',''')+''')  )'

if @finyear='06_07'
select @QR_join=' inner join webx_acctinfo a on t.acccode=a.acccode left outer join webx_location l on l.loccode=t.brcd inner join webx_groups g on g.groupcode=a.groupcode'
else 
select @QR_join=' inner join webx_acctinfo a on t.acccode=a.acccode inner join webx_location l on l.loccode=t.brcd inner join webx_groups g on g.groupcode=a.groupcode'

If @RPT='1'
	 --@QR_join=@QR_join+' inner join webx_groups c on c.groupcode=a.groupcode',
	select @TYP_Select ='Main_category,GRP=(case when grouplevel='+char(39)+'4'+char(39)+' then (select (isnull(company_groupcode,groupcode)+'+char(39)+' : '+char(39)+'+groupdesc) from webx_groups where groupcode=t.PARENTCODE)         
else (isnull(company_groupcode,t.groupcode)+'+char(39)+' : '+char(39)+'+groupdesc) end ),RPTCD=t.groupcode,RPTNM=isnull(t.company_groupcode,t.groupcode)+'' : ''+groupdesc',
	@TYP='Main_category,groupdesc,t.groupcode,t.company_groupcode,grouplevel,PARENTCODE',@TYP_SubRQ='Main_category,groupdesc,a.groupcode,g.company_groupcode,grouplevel,PARENTCODE'
else If @RPT='2'
	select @TYP_Select ='RPTCD=loccode,RPTNM=loccode+'' : ''+locname',
	@TYP='locname,loccode',@TYP_SubRQ='locname,loccode'
else If @RPT='3'
	select @TYP_Select ='RPTCD=t.Acccode,RPTNM=isnull(company_Acccode,t.Acccode)+'' : ''+Accdesc',
	@TYP='Accdesc,t.Acccode,Company_Acccode',@TYP_SubRQ='Accdesc,t.Acccode,Company_Acccode'
else If @RPT='4'
	select @TYP_Select ='RPTCD=custcd,RPTNM=custcd+'' : ''+custnm',
	@TYP='custcd,custnm',@TYP_SubRQ='custcd,custnm',
	@QR_join=@QR_join+' inner join webx_custhdr c on t.pbov_code=c.custcd'
else If @RPT='5'
	select @TYP_Select ='RPTCD=Vendorcode,RPTNM=Vendorcode+'' : ''+Vendorname',
	@TYP='Vendorname,Vendorcode',@TYP_SubRQ='Vendorname,Vendorcode',
	@QR_join=@QR_join+' inner join webx_vendor_hdr c on t.pbov_code=c.Vendorcode'
else If @RPT='6'
	select @TYP_Select ='RPTCD=userid,RPTNM=userid+'' : ''+Name',
	@TYP='Name,userid',@TYP_SubRQ='Name,userid',
	@QR_join=@QR_join+' inner join webx_master_users c on t.pbov_code=c.userid'
else If @RPT='7'
	select @TYP_Select ='RPTCD=manual_driver_code,RPTNM=varchar,manual_driver_code+'' : ''+driver_name',
	@TYP='driver_name,manual_driver_code=convert(varchar,manual_driver_code),driver_id',@TYP_SubRQ='driver_name,manual_driver_code=convert(varchar,manual_driver_code),driver_id',
	@QR_join=@QR_join+' inner join WEBX_FLEET_DRIVERMST c on t.pbov_code=convert(varchar,manual_driver_code)'


if @RPT_SUB='1'
	select @TYP_Select =@TYP_Select+',RPTCD_SUB=t.Acccode,RPTNM_SUB=isnull(company_Acccode,t.Acccode)+'' : ''+Accdesc',
	@TYP=@TYP+',Accdesc,t.Acccode,company_Acccode',@TYP_SubRQ=@TYP_SubRQ+',Accdesc,t.Acccode,Company_Acccode'
else if @RPT_SUB='2'
	select @TYP_Select =@TYP_Select+',RPTCD_SUB=loccode,RPTNM_SUB=loccode+'' : ''+locname',
	@TYP=@TYP+',locname,loccode',@TYP_SubRQ=@TYP_SubRQ+',locname,loccode'
	
If @RPT='1'  or @RPT='2' or  @RPT='3' 
	select @SQL_OPN=' select t.* from (select credit=opencredit_i,debit=opendebit_i,t.brcd,acccode from webx_acctopening_'+@finyear+'  t  With(NOLOCK) )t '
else 
	select @SQL_OPN=' select t.* from (select credit=opencredit,debit=opendebit,brcd=t.loccode,pbov_code=empcd,acccode from webx_acctopening_Employee_'+@finyear+' t  With(NOLOCK) )t '

--select @SQL_OPN

if charindex('OPN',@OPN_TOT_CL_BL)&gt;0 --For opening
	select @SQL_STR=@SQL_STR+',Tot_Opn_Dr=sum(isnull(OPNdebit,0)),Tot_Opn_Cr=sum(isnull(OPNcredit,0)),
Diff_Opn_Dr=(case when sum(isnull(OPNdebit,0))&gt;sum(isnull(OPNcredit,0)) then sum(isnull(OPNdebit,0))-sum(isnull(OPNcredit,0)) else 0 end),
Diff_Opn_Cr=(case when sum(isnull(OPNdebit,0))&gt;sum(isnull(OPNcredit,0)) then 0 else sum(isnull(OPNcredit,0))-sum(isnull(OPNdebit,0)) end)'

if charindex('TOT',@OPN_TOT_CL_BL)&gt;0 --For Total 
	select @SQL_STR=@SQL_STR+',Tot_Dr=sum(isnull(debit,0)),Tot_Cr=sum(isnull(credit,0)),
Diff_Tot_Dr=(case when sum(isnull(debit,0))&gt;sum(isnull(credit,0)) then sum(isnull(debit,0))-sum(isnull(credit,0)) else 0 end),
Diff_Tot_Cr=(case when sum(isnull(debit,0))&gt;sum(isnull(credit,0)) then 0 else sum(isnull(credit,0))-sum(isnull(debit,0)) end)'

if charindex('CL',@OPN_TOT_CL_BL)&gt;0 --FOR Closeing
	select @SQL_STR=@SQL_STR+',Tot_CL_Dr=(case when sum(isnull(OPNdebit,0))+sum(isnull(debit,0))&gt;sum(isnull(OPNcredit,0))+sum(isnull(credit,0)) then sum(isnull(OPNdebit,0))+sum(isnull(debit,0))-sum(isnull(OPNcredit,0))-sum(isnull(credit,0)) else 0 end),
Tot_CL_Cr=(case when sum(isnull(OPNdebit,0))+sum(isnull(debit,0))&gt;sum(isnull(OPNcredit,0))+sum(isnull(credit,0)) then 0 else sum(isnull(OPNcredit,0))+sum(isnull(credit,0))-sum(isnull(OPNdebit,0))-sum(isnull(debit,0)) end)'

if charindex('BL',@OPN_TOT_CL_BL)&gt;0 --For Bal Amt
	select @SQL_STR=@SQL_STR+',Final_Diff= sum(isnull(OPNdebit,0))+sum(isnull(debit,0))-sum(isnull(OPNcredit,0))-sum(isnull(credit,0)) '


	--select @SQL_STR=@SQL_STR+',Final_Diff=(case when sum(isnull(OPNdebit,0))+sum(isnull(debit,0))&gt;sum(isnull(OPNcredit,0))+sum(isnull(credit,0)) then sum(isnull(OPNdebit,0))+sum(isnull(debit,0))-sum(isnull(OPNcredit,0))-sum(isnull(credit,0)) --else sum(isnull(OPNcredit,0))+sum(isnull(credit,0))-sum(isnull(OPNdebit,0))-sum(isnull(debit,0)) end)'

select @SQL='select  '+@TYP_Select+@SQL_STR+'
from (
/*select  '+@TYP_SubRQ+',OPNdebit= sum(isnull(debit,0)),OPNcredit= sum(isnull(credit,0)) ,debit=0,credit=0*/
select  '+@TYP_SubRQ+',
OPNdebit= case when sum(isnull(debit,0))-sum(isnull(credit,0))&gt;0 then sum(isnull(debit,0))-sum(isnull(credit,0)) else 0 end,
OPNcredit= case when sum(isnull(credit,0))-sum(isnull(debit,0))&gt;0 then sum(isnull(credit,0))-sum(isnull(debit,0)) else 0 end,
debit=0,credit=0
from ('+@SQL_OPN+')t
'+@QR_join+' where  1=1 '+@whereACC+'
group by '+@TYP_SubRQ+'
union
select '+@TYP_SubRQ+',
OPNdebit=sum(case when convert(varchar,transdate,106) between convert(datetime,''01 apr '+left(@finyear,2)+''',106) and convert(datetime,'''+@fromdt+''',106)-1
then isnull(debit,0) end),
OPNcredit=sum(case when convert(varchar,transdate,106) between convert(datetime,''01 apr '+left(@finyear,2)+''',106) and convert(datetime,'''+@fromdt+''',106)-1
then isnull(credit,0) end),
debit= sum(isnull(debit,0)),credit= sum(isnull(credit,0))
from webx_acctrans_'+@finyear+' t 
'+@QR_join+'
where isnull(voucher_cancel,''N'')=''N'' '+@whereACC+'
and convert(varchar,transdate,106) between convert(datetime,'''+@fromdt+''',106) and convert(datetime,'''+@todt+''',106)
group by '+@TYP_SubRQ+'

)t
group by '+@TYP+'
order by '+@TYP+'
'
exec(@SQL)
select @SQL
end</CommandText>
