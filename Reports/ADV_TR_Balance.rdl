    <DataSet Name="ACCC">
      <Fields>
        <Field Name="ACC">
          <DataField>ACC</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SSRS</DataSourceName>
        <CommandText>declare @AADESC varchar(1000)
select  @AADESC='('''+replace(@acccode,',',''',''')+''')'

exec('SELECT ACC=ACC_1+(case when  isnull(ACC_2,'''')='''' then '''' else '' , ''+ACC_2 end)
+(case when  isnull(ACC_3,'''')='''' then '''' else '' , ''+ACC_3 end)+(case when  isnull(ACC_4,'''')='''' then '''' else '' , ''+ACC_4 end)+(case when  isnull(ACC_5,'''')='''' then '''' else '' , ''+ACC_5 end)
+(case when  isnull(ACC_6,'''')='''' then '''' else '' , ''+ACC_6 end)+(case when  isnull(ACC_7,'''')='''' then '''' else '' , ''+ACC_7 end)+(case when  isnull(ACC_8,'''')='''' then '''' else '' , ''+ACC_8 end)
+(case when  isnull(ACC_9,'''')='''' then '''' else '' , ''+ACC_9 end)+(case when  isnull(ACC_10,'''')='''' then '''' else '' , ''+ACC_10 end) FROM       
(      SELECT  TYP,ADESC, ''ACC_'' + CAST(          RANK() OVER (      
    PARTITION BY TYP       ORDER BY accdesc) AS VARCHAR(10)) AccList      
FROM   (select acccode,company_acccode,accdesc,TYP=''A'',ADESC=isnull(company_acccode,acccode)+'' : ''+accdesc from webx_acctinfo with(NOLOCK)  where acccode in '+ @AADESC+' or company_acccode in '+ @AADESC+'
 )a  )p      
PIVOT       
    (MAX(ADESC) FOR AccList IN       
        (ACC_1, ACC_2,ACC_3,ACC_4,ACC_5,ACC_6,ACC_7,ACC_8,ACC_9,ACC_10,ACC_11)      
    ) AS ACC '   )</CommandText>
        <QueryParameters>
          <QueryParameter Name="@acccode">
            <Value>=Parameters!acccode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
